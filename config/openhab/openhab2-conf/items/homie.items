Group homie "Homie"


String wemos2d1_name "Homie name: [%s]" { mqtt="<[nasmqtt:homie/5ccf7f2c12d1/$name:state:REGEX((.*))]" }
Switch wemos2d1_online "Homie przelacznik online: [%s]" (gReachable) { mqtt="<[nasmqtt:homie/5ccf7f2c12d1/$online:state:JS(homie-tf.js)"}

Switch  wemos2d1_switch "relay" <switch> (SonoffBedLight,HouseLight) { 
		mqtt=">[nasmqtt:homie/5ccf7f2c12d1/1/on/set:command:ON:true],
			  >[nasmqtt:homie/5ccf7f2c12d1/1/on/set:command:OFF:false],
  			  <[nasmqtt:homie/5ccf7f2c12d1/1/on:state:JS(homie-tf.js)]
  			  "
}
Number	wemos2d1_tempReadout	"Readout temperature [%.1f °C]"	<temperature> {
	mqtt="<[nasmqtt:homie/5ccf7f2c12d1/currentTemp/degrees:state:default]"
}

Number	wemos2d1_tempDesired	"Desired temperature [%.1f °C]"	<temperature> {
	mqtt="
		<[nasmqtt:homie/5ccf7f2c12d1/desiredTemp/degrees:state:default],
 		>[nasmqtt:homie/5ccf7f2c12d1/desiredTemp/degrees/set:command:*:default]

		"
}


